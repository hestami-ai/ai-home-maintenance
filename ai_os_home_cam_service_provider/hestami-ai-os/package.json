{
	"name": "hestami-ai-os",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"db:generate": "prisma generate",
		"db:migrate": "prisma migrate dev",
		"db:migrate:deploy": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:seed": "tsx prisma/seed.ts",
		"db:studio": "prisma studio",
		"db:reset": "prisma migrate reset",
		"openapi:generate": "tsx scripts/generate-openapi.ts",
		"types:generate": "openapi-typescript ./openapi.json -o ./src/lib/api/types.generated.ts",
		"prebuild": "npm run openapi:generate && npm run types:generate"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@playwright/test": "^1.56.1",
		"@skeletonlabs/skeleton": "^4.8.0",
		"@skeletonlabs/skeleton-svelte": "^4.8.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^24.10.1",
		"@types/pg": "^8.15.6",
		"@types/swagger-ui-dist": "^3.30.6",
		"@typescript/analyze-trace": "^0.10.1",
		"@vitest/browser-playwright": "^4.0.10",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"openapi-typescript": "^7.10.1",
		"playwright": "^1.56.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"prisma": "^7.1.0",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"sveltekit-superforms": "^2.29.1",
		"tailwindcss": "^4.1.17",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.47.0",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.10",
		"vitest-browser-svelte": "^2.0.1",
		"zod-prisma-types": "file:zod-prisma-types-3.5.8.tgz"
	},
	"dependencies": {
		"@cerbos/grpc": "^0.23.5",
		"@dbos-inc/dbos-sdk": "^4.5.13",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/auto-instrumentations-node": "^0.67.2",
		"@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
		"@opentelemetry/resources": "^2.2.0",
		"@opentelemetry/sdk-node": "^0.208.0",
		"@opentelemetry/semantic-conventions": "^1.38.0",
		"@orpc/client": "^1.13.0",
		"@orpc/openapi": "^1.12.2",
		"@orpc/server": "^1.12.2",
		"@orpc/zod": "^1.12.2",
		"@prisma/adapter-pg": "^7.1.0",
		"@prisma/client": "^7.1.0",
		"better-auth": "^1.4.5",
		"dotenv": "^17.2.3",
		"flowbite-svelte": "^1.31.0",
		"ioredis": "^5.8.2",
		"lucide-svelte": "^0.561.0",
		"nanoid": "^5.1.6",
		"pg": "^8.16.3",
		"swagger-ui-dist": "^5.31.0",
		"zod": "^4.1.13"
	}
}
